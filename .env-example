# Example environment variables for GeoJSON Backend

# Database connection string
DATABASE_URL=postgresql://postgres:postgres@db:5432/mydb

# Secret key for JWT
SECRET_KEY=your_secret_key_here

# JWT access token expiry (in minutes)
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Lepton Maps API key
LEPTON_API_KEY=your_leptonmaps_api_key_here

# Environment (set to 'development' to enable docs, 'production' to disable)
ENV=development

# CORS allowed origins (comma-separated, e.g. https://yourfrontend.com,https://admin.yourfrontend.com)
CORS_ORIGINS=*

# Global rate limit (e.g. 100/minute)
RATE_LIMIT=100/minute 